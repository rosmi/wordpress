
{
  "containerDefinitions": [
    {
      "name": "web",
      "links": [
        "db"
      ],
      "image": "wordpress1",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "memory": 300,
      "cpu": 10
    },
    {
      "environment": [
        {
          "name": "MYSQL_DATABASE",
          "value": "wordpress"
        },
        {
          "name": "MYSQL_ROOT_PASSWORD",
          "value":"test"
        }
        
      ],
      "name": "db",
      "image": "mysql",
      "cpu": 10,
      "memory": 200,
      "essential": true
    }
  ],
  "family": "hello_world"
}
